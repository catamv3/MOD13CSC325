<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="background" prefHeight="492.0" prefWidth="496.0" styleClass="anchorbackground" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csc325_firebase_webview_auth.modelview.AccessFBView">
   <children>
      <VBox alignment="CENTER_RIGHT" layoutX="7.0" prefHeight="290.0" prefWidth="485.0" spacing="10.0">
         <children>
            <TextField fx:id="tfUsername" layoutX="20.0" layoutY="161.0" promptText="Username" />
            <TextField fx:id="tfName" alignment="TOP_LEFT" promptText="Name" />
            <TextField fx:id="tfMajor" promptText="Major" />
            <TextField fx:id="tfAge" promptText="Age" />
            <TextField fx:id="tfPassword" layoutX="20.0" layoutY="24.0" promptText="Password" />
            <TextField fx:id="tfConfirmPassword" layoutX="20.0" layoutY="55.0" promptText="Confirm Password" />
            <AnchorPane prefHeight="91.0" prefWidth="465.0">
               <children>
                  <Button fx:id="switchroot" layoutX="13.0" layoutY="15.0" mnemonicParsing="false" onAction="#switchToSecondary" prefHeight="25.0" prefWidth="65.0" text="Switch" textAlignment="CENTER" />
                  <Button fx:id="readButton" layoutX="110.0" layoutY="15.0" mnemonicParsing="false" onAction="#readRecord" prefHeight="25.0" prefWidth="90.0" text="Read" textAlignment="CENTER" />
                  <Button fx:id="writeButton" layoutX="223.0" layoutY="15.0" mnemonicParsing="false" onAction="#addRecord" prefHeight="25.0" prefWidth="81.0" text="Write" textAlignment="CENTER" />
                  <Button fx:id="regButton" layoutX="327.0" layoutY="15.0" mnemonicParsing="false" onAction="#regRecord" prefHeight="25.0" prefWidth="113.0" text="Register" textAlignment="CENTER" />
               </children>
            </AnchorPane>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <TextArea fx:id="outputField" layoutX="7.0" layoutY="313.0" prefHeight="167.0" prefWidth="485.0" styleClass="textView" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
